   "source": [
    "import tkinter as tk\n",
    "\n",
    "# Tạo cửa sổ chính\n",
    "root = tk.Tk()\n",
    "\n",
    "# Đặt tiêu đề cho cửa sổ\n",
    "root.title(\"Cửa sổ GUI với Tkinter\")\n",
    "\n",
    "# Thiết lập kích thước cửa sổ (tuỳ chọn)\n",
    "root.geometry(\"400x300\")  # Rộng 400, cao 300\n",
    "\n",
    "# Hiển thị cửa sổ\n",
    "root.mainloop()"
   ]
  }
